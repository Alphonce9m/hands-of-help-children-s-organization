# Database
DATABASE_URL="postgresql://user:password@localhost:5432/your_database?schema=public"

# NextAuth
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=https://your-production-url.com

# M-Pesa (if applicable)
